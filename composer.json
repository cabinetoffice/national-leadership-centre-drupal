{
    "name": "cabinetoffice/national-leadership-centre-drupal",
    "description": "NLC Connect With Your Peers site, based on Drupal Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Joe Baker",
            "role": "Technical Lead"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.2",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/address": "~1.0",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/bootstrap_barrio": "^4.22",
        "drupal/coder": "^8.3",
        "drupal/collapsiblock": "^2.0",
        "drupal/config_filter": "2.x-dev",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/devel": "^2.1",
        "drupal/elasticsearch_connector": "^6.0@alpha",
        "drupal/email_registration": "^1.0@RC",
        "drupal/encrypt": "^3.0@RC",
        "drupal/eu_cookie_compliance": "^1.8",
        "drupal/facets": "^1.4",
        "drupal/field_group": "^3.0",
        "drupal/google_analytics": "^3.0",
        "drupal/govuk_design_system": "^1.0@beta",
        "drupal/health_check": "^1.2",
        "drupal/htmlmail": "^3.0@alpha",
        "drupal/key": "^1.8",
        "drupal/migrate_plus": "^4.2",
        "drupal/migrate_source_csv": "^3.0",
        "drupal/migrate_tools": "^4.1",
        "drupal/name": "^1.0@RC",
        "drupal/profile": "^1.0@RC",
        "drupal/real_aes": "^2.2",
        "drupal/remove_http_headers": "^1.0@beta",
        "drupal/s3fs": "^3.0@alpha",
        "drupal/salesforce": "4.x-dev",
        "drupal/search_api": "^1.14",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/select_or_other": "^1.0@alpha",
        "drupal/sendgrid_integration": "^1.2",
        "drupal/structure_sync": "^1.16",
        "drupal/token": "^1.5",
        "drupal/userprotect": "^1.0",
        "drush/drush": "^10.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
                "web-root": "web/"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patches": {
            "drupal/core": {
                "Notice: Undefined index: value in Drupal\\views\\Plugin\\views\\filter\\NumericFilter->acceptExposedInput()": "https://www.drupal.org/files/issues/2019-04-17/exposed-filter-notice-2825860-25.patch"
            },
            "drupal/google_analytics": {
                "Allow event arguments in page attachments to be altered before being encoded to JSON": "https://www.drupal.org/files/issues/2020-03-02/ga-alter-event-arguments-in-page-attachments-3117147-01.patch"
            },
            "drupal/salesforce": {
                "Refresh SF auth token drush command should also store the identity on first token request": "https://www.drupal.org/files/issues/2020-03-16/sf-refresh-auth-token-get-identity-also-3120102-2.patch"
            }
        }
    }
}
